{
  "name": "biiiiiigmonster/relation",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "relation component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "swoft/db": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "BiiiiiigMonster\\Relation\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "BiiiiiigMonster\\RelationTest\\Unit\\": "test/unit/",
      "BiiiiiigMonster\\RelationTest\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
